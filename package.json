{
  "name": "@hemyn/egg-rpc",
  "version": "0.0.1",
  "description": "rpc base plugin for eggjs",
  "eggPlugin": {
    "name": "rpc"
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin"
  ],
  "dependencies": {
    "antpb": "^1.0.0",
    "is-type-of": "^1.2.1",
    "pump": "^3.0.0",
    "sofa-bolt-node": "^1.1.1",
    "sofa-rpc-node": "^1.6.2"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg": "^2.14.2",
    "egg-bin": "^4.10.0",
    "egg-mock": "^3.21.0",
    "egg-rpc-generator": "^1.2.0",
    "eslint": "^5.12.0",
    "eslint-config-egg": "^7.1.0",
    "mz-modules": "^2.1.0",
    "standard-version": "^9.3.2",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "autod": "autod",
    "lint": "eslint . --ext .js",
    "cov": "sh test/init.sh && TEST_TIMEOUT=20000 egg-bin cov",
    "test": "npm run lint && npm run test-local",
    "test-local": "sh test/init.sh && egg-bin test",
    "pkgfiles": "egg-bin pkgfiles --check",
    "ci": "npm run autod -- --check && npm run pkgfiles && npm run lint && npm run cov",
    "contributors": "contributors -f plain -o AUTHORS",
    "release": "standard-version"
  },
  "files": [
    "app",
    "config",
    "lib",
    "app.js",
    "agent.js"
  ],
  "ci": {
    "version": "8, 10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Foreinyel/egg-rpc.git"
  },
  "homepage": "https://github.com/Foreinyel/egg-rpc#readme",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
